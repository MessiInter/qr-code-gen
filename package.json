{
  "name": "@qr-code-gen/source",
  "version": "0.0.0-next.0",
  "private": true,
  "description": "QR Code Generator is a application that can quickly generate QR Code for general purposes uses",
  "bin": {
    "commit-status": "./bin/commit-status.mjs"
  },
  "keywords": [
    "QR",
    "Generator",
    "QR Code",
    "QR Code Generator",
    "qr-code-gen",
    "Angular",
    "nx"
  ],
  "scripts": {
    "start": "nx serve",
    "start:static": "nx serve-static",
    "start:express": "node ./dist/qr-code-gen/server/server.mjs",
    "build": "nx build",
    "build:watch": "nx build --watch --configuration=development",
    "test": "nx test",
    "test:watch": "nx test --watchAll",
    "e2e": "nx e2e e2e",
    "e2e:watch": "nx e2e e2e --watch",
    "lint": "pnpm \"/lint:check:.*/\"",
    "lint:fix": "pnpm \"/lint:fix:.*/\"",
    "lint:check:code": "nx lint --reportUnusedDisableDirectives=warn",
    "lint:check:e2e": "nx lint --reportUnusedDisableDirectives=warn e2e",
    "lint:check:style": "stylelint \"**/*.css\"",
    "lint:fix:code": "nx lint --cache --fix",
    "lint:fix:e2e": "nx lint --cache --fix e2e",
    "lint:fix:style": "stylelint --cache --fix \"**/*.css\"",
    "format": "prettier --check .",
    "format:code": "nx format:write --all",
    "deploy": "pnpm predeploy && git subtree push --prefix dist/qr-code-gen/browser origin gh-pages",
    "predeploy": "nx build --baseHref=/qr-code-gen/",
    "postinstall": "concurrently -p \"[{name}]\" -c \"bgGray.bold,bgYellow.bold\" -n \"HUSKY,PNPM\" \"husky install\" \"pnpm link -g\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MessiInter/qr-code-gen.git"
  },
  "engines": {
    "node": ">=18.13.0",
    "npm": "Please use pnpm instead of npm to install dependencies",
    "yarn": "Please use pnpm instead of yarn to install dependencies"
  },
  "author": "qr-code-gen authors",
  "contributors": [
    {
      "name": "MessiInter",
      "email": "messivsinter@gmail.com",
      "url": "https://github.com/MessiInter",
      "author": true
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MessiInter/qr-code-gen/issues",
    "email": "messivsinter@gmail.com"
  },
  "homepage": "https://messiinter.github.io/qr-code-gen",
  "dependencies": {
    "@angular/animations": "~17.0.0",
    "@angular/cdk": "^17.0.2",
    "@angular/common": "~17.0.0",
    "@angular/compiler": "~17.0.0",
    "@angular/core": "~17.0.0",
    "@angular/forms": "~17.0.0",
    "@angular/material": "^17.0.2",
    "@angular/platform-browser": "~17.0.0",
    "@angular/platform-browser-dynamic": "~17.0.0",
    "@angular/platform-server": "~17.0.0",
    "@angular/router": "~17.0.0",
    "@angular/ssr": "~17.0.0",
    "@nx/angular": "17.1.3",
    "angularx-qrcode": "^17.0.0",
    "express": "~4.18.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~17.0.0",
    "@angular-devkit/core": "~17.0.0",
    "@angular-devkit/schematics": "~17.0.0",
    "@angular-eslint/eslint-plugin": "~17.0.0",
    "@angular-eslint/eslint-plugin-template": "~17.0.0",
    "@angular-eslint/template-parser": "~17.0.0",
    "@angular/cli": "~17.0.0",
    "@angular/compiler-cli": "~17.0.0",
    "@angular/language-service": "~17.0.0",
    "@nx/cypress": "17.1.3",
    "@nx/eslint": "17.1.3",
    "@nx/eslint-plugin": "17.1.3",
    "@nx/jest": "17.1.3",
    "@nx/js": "17.1.3",
    "@nx/web": "17.1.3",
    "@nx/workspace": "17.1.3",
    "@schematics/angular": "~17.0.0",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "~1.3.85",
    "@types/express": "4.17.14",
    "@types/jest": "^29.4.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "concurrently": "^8.2.2",
    "cypress": "^13.0.0",
    "eslint": "~8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.13.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "git-auto-commit-msg": "^0.1.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-preset-angular": "~13.1.4",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^15.2.0",
    "nx": "17.1.3",
    "prettier": "^2.6.2",
    "stylelint": "^16.1.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.2.2"
  },
  "packageManager": "pnpm@8.13.1",
  "lint-staged": {
    "*.css": "stylelint --allow-empty-input --cache --fix",
    "*.{js,ts,mjs,html}": "eslint --cache --fix",
    "*.{js,ts,mjs,html,css,md,json}": "prettier --cache --write"
  }
}
