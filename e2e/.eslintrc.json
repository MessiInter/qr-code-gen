{
  "extends": [
    "plugin:cypress/recommended",
    "../.eslintrc.json",
    "../.eslintrc.base.json"
  ],
  "ignorePatterns": ["!**/*"],
  "plugins": ["@nx"],
  "overrides": [
    {
      "files": ["*.{ts,js,cjs,mjs,tsx,jsx}"],
      "rules": {}
    },
    {
      "files": ["*.{ts,tsx}"],
      "rules": {}
    },
    {
      "files": [
        "*.cy.{ts,js,cjs,mjs,tsx,jsx}",
        "src/**/*.{ts,js,cjs,mjs,tsx,jsx}"
      ],
      "rules": {}
    },
    {
      "files": ["*.{js,cjs,mjs,jsx}"],
      "rules": {}
    }
  ]
}
